		</tr>
		<tr>
			<td>Rent </td>
			<td><%= @report.rent%></td>
		</tr>
		<tr>
			<td>Depreciation</td>
			<td><%= @report.depreciation%></td>
		</tr>
		<tr>
			<td>Tax</td>
			<td><%= @report.tax%></td>
		</tr>
		<tr>
			<td>Net income</td>
			<td><%= @report.net_income%></td>
