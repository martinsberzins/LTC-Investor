		<tr>
			<td>Goodwill</td>
			<td><%= @report.goodwill%></td>
		</tr>
		<tr>
			<td>Fixed assets</td>
			<td><%= @report.fixed_assets%></td>
		</tr>
		<tr>
			<td>Current liabilities</td>
			<td><%= @report.curr_liab%></td>
		</tr>
		<tr>
			<td>Long term debt</td>
			<td><%= @report.lt_debt%></td>
		</tr>
		<tr>
			<td>Equity</td>
			<td><%= @report.equity%></td>
		</tr>
