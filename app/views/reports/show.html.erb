

<table class="table table-condensed">
	<h1> <%= @post.name%> </h1>

	<thead>
		<tr>
			<th>Quarter</th>
			<th><%= @report.quarter%></th>
		</tr>
	</thead>
	<tbody>
		<tr>
			<td>Date of report </td>
			<td><%= @report.date_of_report%></td>
		</tr>	
		<tr>
			<td>Date of report </td>
			<td><%= @report.date_of_report%></td>
		</tr>	
		<tr>
			<td>Units </td>
			<td><%= @report.units%></td>
		</tr>	
		<tr>
			<td>Shares outstanding</td>
			<td><%= @report.shares_outstanding%></td>
		</tr>	
		<tr>
			<td>High price</td>
			<td><%= @report.high_price%></td>
		</tr>	
		<tr>
			<td>Low price </td>
			<td><%= @report.low_price%></td>
		</tr>	
